{
	"env": {
		"development": {
			"presets": [
				"react",
				["es2015", {"modules": "commonjs"}],
				"stage-0"
			],
			"plugins": [
				"react-hot-loader/babel",
				"add-module-exports",
				"transform-decorators-legacy",
				"transform-react-display-name"
			]
		},
		"production": {
			"presets": [
				["es2015", {"modules": false}],
				"react",
				"stage-0"
			],
			"plugins": [
				"syntax-export-extensions",
				"transform-export-extensions",
				"transform-decorators-legacy",
				"transform-react-display-name"
			]
		},
		"es6": {
			"presets": [
				["es2015", {"modules": false}],
				"react",
				"stage-0"
			],
			"plugins": [
				"react-hot-loader/babel",
				"syntax-export-extensions",
				"transform-export-extensions",
				"transform-decorators-legacy",
				"transform-react-display-name"
			]
		}
	}
}