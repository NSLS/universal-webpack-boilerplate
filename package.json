  "scripts": {
    "dev": "run-p development-build-server development-build-client development-prepare-server-build development-server-delayed",
    "development-prepare-server-build": "universal-webpack --settings ./webpack/universal-webpack-settings.js prepare",
    "development-build-client": "babel-node ./webpack/webpack-dev-server.js",
    "development-build-server": "better-npm-run development-build-server",
    "development-server": "better-npm-run development-server",
    "development-server-delayed": "run-s delay development-server",
    "delay": "node ./src/core/sleep 10000",
    "production": "npm-run-all production-build-client production-build-server production-server",
    "production-build-client": "webpack --colors --display-error-details --config ./webpack/webpack.config.client.production.babel.js",
    "production-build-server": "webpack --colors --display-error-details --config ./webpack/webpack.config.server.production.babel.js",
    "production-server": "better-npm-run production-server",
    "build-dll": "webpack --config ./webpack/webpack.dll.config.babel.js --colors --display-error-details"
  },
  "betterScripts": {
    "development-build-server": {
      "env": {
        "NODE_PATH": "./src"
      },
      "command": "webpack --config ./webpack/webpack.config.server.development.babel.js --watch --colors --display-error-details"
    },
    "development-server": {
      "env": {
        "NODE_PATH": "./src"
      },
      "command": "nodemon ./src/core/server/start.js --watch ./webroot/build/client"
    },
    "production-server": {
      "env": {
        "NODE_PATH": "./src"
      },
      "command": "node ./src/core/server/start.js"
    }
  },
